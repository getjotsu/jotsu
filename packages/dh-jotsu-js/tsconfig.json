{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
        "declaration": false,
        "jsx": "react-jsx",
        "jsxImportSource": "preact",
        "paths": {
            "react": [
                "../../../node_modules/preact/compat"
            ],
            "react/*": [
                "../../../node_modules/preact/compat/*"
            ],
            "react/jsx-runtime": [
                "../../../node_modules/preact/jsx-runtime"
            ],
            "react-dom": [
                "../../../node_modules/preact/compat"
            ],
            "react-dom/*": [
                "../../../node_modules/preact/compat/*"
            ]
        },
        "baseUrl": "src"
    },
    "include": [
        "src"
    ]
}
